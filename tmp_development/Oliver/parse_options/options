Assumption: -f and -t can only be specified seperately
-> Valid combinations:
h
f and o
t and o
o
f
t

Valid arguments:
none
- 
input1 through inputn

argc = 1

	./mc_ast_to_dot

argc = 2

	./mc_ast_to_dot -
	./mc_ast_to_dot -h
	./mc_ast_to_dot input1

argc = 3

	./mc_ast_to_dot -t input1
	./mc_ast_to_dot -t -
	./mc_ast_to_dot input1 input2

argc = 4

	./mc_ast_to_dot -o output -
	./mc_ast_to_dot -o output input1
	./mc_ast_to_dot -t input1 input2
	./mc_ast_to_dot -f function input1
	./mc_ast_to_dot	-f function -
	./mc_ast_to_dot input1 input2 input3

argc = 5

	./mc_ast_to_dot -o output -t input1 									+ permutations
	./mc_ast_to_dot -o output -t - 												+ permutations  
	./mc_ast_to_dot -t input1 input2 input3
	./mc_ast_to_dot -o output input1 input2
	./mc_ast_to_dot -f function input1 input2 
	./mc_ast_to_dot -f function -o output input1 					+ permutations
	./mc_ast_to_dot -f function -o output -								+ permutations

argc = 6

	./mc_ast_to_dot -o output input1 input2 input3
	./mc_ast_to_dot -f function input1 input2 input3
	./mc_ast_to_dot -t input1 input2 input3 input4
	./mc_ast_to_dot -o output -f function -								+ permutations 
	./mc_ast_to_dot -o output -f function input1					+ permutations 
	./mc_ast_to_dot -o output -t input1 input2						+ permutations 
	./mc_ast_to_dot input1 input2 input3 input4 input5
	
argc > 7

  ./mc_ast_to_dot -o output -f function inputs 					+ permutations 
	./mc_ast_to_dot -o output inputs
	./mc_ast_to_dot -f function inputs
	./mc_ast_to_dot -o output -t inputs										+ permutations 
	./mc_ast_to_dot	-t inputs
	./mc_ast_to_dot	inputs
